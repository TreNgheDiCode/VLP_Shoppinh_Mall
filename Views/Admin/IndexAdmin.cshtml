							<form method="post" class="modal-body" asp-controller="ChiNhanh" asp-action="Create" enctype="multipart/form-data">
								<input type="hidden" asp-for="@Model.chiNhanhViewModel.UserId" />

								<div class="my-2 form-group">
									<label asp-for="@Model.chiNhanhViewModel.TenChiNhanh">Tên chi nhánh</label>
									<input asp-for="@Model.chiNhanhViewModel.TenChiNhanh" type="text" class="form-control" placeholder="Tên chi nhánh">
									<span asp-validation-for="@Model.chiNhanhViewModel.TenChiNhanh" class="text-danger"></span>
								</div>
								<div class="my-2 form-group">
									<label asp-for="@Model.chiNhanhViewModel.NoiDung">Nội dung</label>
									<input asp-for="@Model.chiNhanhViewModel.NoiDung" type="text" class="form-control" placeholder="Nội dung">
									<span asp-validation-for="@Model.chiNhanhViewModel.NoiDung" class="text-danger"></span>
								</div>
								<div class="my-2 form-group">
									<label asp-for="@Model.chiNhanhViewModel.AnhDaiDien">Ảnh đại diện</label>
									<input asp-for="@Model.chiNhanhViewModel.AnhDaiDien" type="file" class="form-control" placeholder="Ảnh đại diện">
									<span asp-validation-for="@Model.chiNhanhViewModel.AnhDaiDien" class="text-danger"></span>
								</div>
								<div class="my-2 form-group">
									<label asp-for="@Model.chiNhanhViewModel.Email">Email</label>
									<input asp-for="@Model.chiNhanhViewModel.Email" type="email" class="form-control" placeholder="Email">
									<span asp-validation-for="@Model.chiNhanhViewModel.Email" class="text-danger"></span>
								</div>
								<div class="my-2 form-group">
									<label asp-for="@Model.chiNhanhViewModel.SoDienThoai">Số điện thoại</label>
									<input asp-for="@Model.chiNhanhViewModel.SoDienThoai" type="text" class="form-control" placeholder="Số điện thoại">
									<span asp-validation-for="@Model.chiNhanhViewModel.SoDienThoai" class="text-danger"></span>
								</div>
								<div class="my-2 form-group">
									<label asp-for="@Model.chiNhanhViewModel.NgayHoatDong1">Từ</label>
									<select asp-for="@Model.chiNhanhViewModel.NgayHoatDong1">
										<option>--Chọn--</option>
										<option>Thứ Hai</option>
										<option>Thứ Ba</option>
										<option>Thứ Tư</option>
										<option>Thứ Năm</option>
										<option>Thứ Sàu</option>
										<option>Thứ Bảy</option>
										<option>Chủ Nhật</option>
									</select>
									<span asp-validation-for="@Model.chiNhanhViewModel.NgayHoatDong1" class="text-danger"></span>
									<span>&nbsp;-&nbsp;</span>
									<label asp-for="@Model.chiNhanhViewModel.NgayHoatDong2">Đến</label>
									<select asp-for="@Model.chiNhanhViewModel.NgayHoatDong2">
										<option>--Chọn--</option>
										<option>Thứ Hai</option>
										<option>Thứ Ba</option>
										<option>Thứ Tư</option>
										<option>Thứ Năm</option>
										<option>Thứ Sàu</option>
										<option>Thứ Bảy</option>
										<option>Chủ Nhật</option>
									</select>
									<span asp-validation-for="@Model.chiNhanhViewModel.NgayHoatDong2" class="text-danger"></span>
								</div>
								<div class="my-2 form-group">
									<label>Thời gian hoạt động</label>
									<div>Từ</div>
									<input asp-for="@Model.chiNhanhViewModel.ThoiGianHoatDong1" type="time" class="form-control">
									Đến
									<input asp-for="@Model.chiNhanhViewModel.ThoiGianHoatDong2" type="time" class="form-control">
								</div>
								<div class="my-2 form-group">
									<label>Địa chỉ</label>
									<div class="row">
										<div class="col-12">
											<label asp-for="@Model.chiNhanhViewModel.DiaChi.Duong">Tên đường &amp; Số nhà</label>
											<input asp-for="@Model.chiNhanhViewModel.DiaChi.Duong" type="text" class="form-control" placeholder="Tên đường &amp; Số nhà">
											<span asp-validation-for="@Model.chiNhanhViewModel.DiaChi.Duong" class="text-danger"></span>
										</div>
									</div>
									<div class="row">
										<div class="col-6">
											<label asp-for="@Model.chiNhanhViewModel.DiaChi.Phuong">Phường</label>
											<input asp-for="@Model.chiNhanhViewModel.DiaChi.Phuong" type="text" class="form-control" placeholder="Phường">
											<span asp-validation-for="@Model.chiNhanhViewModel.DiaChi.Phuong" class="text-danger"></span>
										</div>
										<div class="col-6">
											<label asp-for="@Model.chiNhanhViewModel.DiaChi.Quan">Quận</label>
											<input asp-for="@Model.chiNhanhViewModel.DiaChi.Quan" type="text" class="form-control" placeholder="Quận">
											<span asp-validation-for="@Model.chiNhanhViewModel.DiaChi.Quan" class="text-danger"></span>
										</div>
									</div>
									<div class="row">
										<div class="col-12">
											<label asp-for="@Model.chiNhanhViewModel.DiaChi.ThanhPho">Tỉnh/Thành phố</label>
											<input asp-for="@Model.chiNhanhViewModel.DiaChi.ThanhPho" type="text" class="form-control" placeholder="Thành phố">
											<span asp-validation-for="@Model.chiNhanhViewModel.DiaChi.ThanhPho" class="text-danger"></span>
										</div>
									</div>
								</div>
								<div class="my-2 form-group">
									<div class="inline w-100 field">
										<label asp-for="@Model.chiNhanhViewModel.CuaHangs">Chọn chi nhánh</label>
										<select asp-for="@Model.chiNhanhViewModel.SelectedCuaHang" asp-items="new SelectList(stores, nameof(CuaHang.Id), nameof(CuaHang.TenCuaHang))" multiple class="label ui selection fluid dropdown">
											<option value="">Chọn</option>

										</select>
									</div>
									<span asp-validation-for="@Model.cuaHangViewModel.ChiNhanhs" class="text-danger"></span>
								</div>
								<div class="modal-footer">
									<button type="button" class="btn btn-secondary" data-dismiss="modal">Hủy</button>
									<button type="submit" class="btn btn-success">Thêm</button>
								</div>
							</form>
						</div>
					</div>
				</div>

